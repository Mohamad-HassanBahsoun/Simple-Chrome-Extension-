{
    "manifest_version": 3,
    "name": "iEatBackground",
    "description": "Add a custom background to google.com",
    "version": "1.0",
    "icons": {
        "16": "/images/16_1.png",
        "48": "/images/48_1.png",
        "128": "/images/128_1.png"
    },
    "action" : {
        "default_icon":{
            "16": "/images/16_2.png",
            "48": "/images/48_2.png",
            "128": "/images/128_2.png"
        }
    },
    "content_scripts": [{
        "matches": ["https://www.google.com/*"],
        "css": ["main.css"]
    }]
}
